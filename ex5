using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<int> numbers = new List<int> { 2, 5, 8, 3, 10 };
        int total = SumNumbers(numbers);
        Console.WriteLine($"Сумма чисел: {total}");
    }

    static int SumNumbers(List<int> numbers)
    {
        int sum = 0;
        foreach(int num in numbers)
        {
            sum += num;
        }
        return sum;
    }
}
